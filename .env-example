# .env file for Anime-Generator-Bot configuration

# Your Bot token from @BotFather
TOKEN=your_bot_token_here

# Your Telegram API ID from https://my.telegram.org/apps
API_ID=your_api_id_here

# Your Telegram API Hash from https://my.telegram.org/apps
API_HASH=your_api_hash_here

# Your Telegram user id from @userinfobot
OWNER=your_owner_id_here

# Port on which the bot runs (default: 8080)
PORT=8080

# Set to True to enable NSFW content; False otherwise
NSFW=False
